    <name>Kilo Browser</name>
    <executable>/opt/kilo/kilo-desktop-wrapper</executable>
    <command>/opt/kilo/kilo-desktop-wrapper %s</command>
    <icon-name>kilo</icon-name>
    <run-in-terminal>false</run-in-terminal>
    <netscape-remote>true</netscape-remote>
    <tab-command>/opt/kilo/kilo-desktop-wrapper %s</tab-command>
    <win-command>/opt/kilo/kilo-desktop-wrapper --new-window %s</win-command>
</web-browser>
